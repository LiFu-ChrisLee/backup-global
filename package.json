{
  "name": "backup-global",
  "version": "1.3.1",
  "description": "Backup your global packages.",
  "scripts": {
    "dev": "ttsc -p tsconfig.dev.json --w",
    "build": "ttsc",
    "start": "node pro/index.js",
    "dev:start": "node dist/index.js",
    "release": "npm version minor && npm publish",
    "release:pre": "npm version preminor && npm publish --tag test",
    "release:major": "npm version major && npm publish",
    "release:major:pre": "npm version premajor && npm publish --tag test"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.5.1"
  },
  "bin": {
    "bkg": "pro/index.js",
    "backup-global": "pro/index.js"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LiFu-ChrisLee/backup-global.git"
  },
  "keywords": [
    "backup",
    "global",
    "packages"
  ],
  "author": "Li Fu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LiFu-ChrisLee/backup-global/issues"
  },
  "homepage": "https://github.com/LiFu-ChrisLee/backup-global#readme",
  "devDependencies": {
    "@types/node": "^8.9.5",
    "@types/ora": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "ttypescript": "^1.5.10",
    "typescript": "^3.8.3",
    "typescript-transform-paths": "^1.1.14"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "commander": "^4.1.1",
    "node-cmd": "^3.0.0",
    "ora": "^4.0.3"
  }
}
